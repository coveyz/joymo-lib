{
	"name": "joymolib-plus",
	"version": "0.0.0",
	"description": "Library for Work",
	"main": "lib/index.js",
	"module": "es/index.mjs",
	"types": "es/index.d.ts",
	"keywords": ["components", "vue"],
	"author": "coveyz",
	"license": "MIT",
  "unpkg": "dist/index.full.js",
	"peerDependencies": {
		"vue": "^3.2.0"
	},
	"devDependencies": {
		"@types/node": "*",
		"csstype": "^2.6.20",
		"vue": "^3.2.37",
		"vue-router": "^4.0.16"
	},
  "dependencies": {
		"@vueuse/core": "^10.2.1"
	},
	"exports": {
		".": {
			"types": "./es/index.d.ts",
			"import": "./es/index.mjs",
			"require": "./lib/index.js"
		},
    "./es": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs"
    },
    "./es/*.mjs": {
      "types": "./es/*.d.ts",
      "import": "./es/*.mjs"
    },
    "./es/*": {
      "types": ["./es/*.d.ts", "./es/*/index.d.ts"],
      "import": "./es/*.mjs"
    },
    "./lib": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js"
    },
    "./lib/*.js": {
      "types": "./lib/*.d.ts",
      "require": "./lib/*.js"
    },
    "./lib/*": {
      "types": ["./lib/*.d.ts", "./lib/*/index.d.ts"],
      "require": "./lib/*.js"
    },
    "./*": "./*"
	},
	"style": "dist/index.css",
  "sideEffects": [
    "dist/*",
    "theme-chalk/**/*.css",
    "theme-chalk/src/**/*.scss",
    "es/components/*/style/*",
    "lib/components/*/style/*"
  ]
}
